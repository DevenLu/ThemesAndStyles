<?xml version="1.0" encoding="utf-8"?>
<resources>

  <declare-styleable name="CheeseView">
    <attr name="sweep_angle" format="float"/>
    <attr name="cheese_type" format="enum">
      <enum name="normal" value="1"/>
      <enum name="dark" value="2"/>
    </attr>

    <attr name="portion_color" format="color">
      <enum name="light" value="-1"/>
      <enum name="dark" value="-2"/>
    </attr>

    <attr name="overlay_text" format="string"/>

    <attr name="rats_fraction" format="fraction"/>

    <attr name="ingredients">
      <flag name="milk" value="1"/>
      <flag name="pepper" value="2"/>
      <flag name="garlic" value="4"/>
      <flag name="thyme" value="8"/>
    </attr>

    <attr name="manufacture_logo" format="reference"/>

    <attr name="rotten" format="boolean"/>

  </declare-styleable>

  <attr name="my_favorite_color" format="color"/>

  <attr name="company_logo" format="reference"/>
  <attr name="company_color" format="color"/>

</resources>